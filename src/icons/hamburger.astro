---
interface Props {
  color: string;
  size: string;
}
const { color, size } = Astro.props;
const iconClass = color + " " + size;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  fill="none"
  id="hamburgerIcon"
  viewBox="0 0 24 24"
  class={iconClass}
  ><path
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="2"
    d="M4 6h16M4 12h16M4 18h16"></path></svg
>
