---
interface Props {
  color: string;
  size: string;
}
const { color, size } = Astro.props;
const iconClass = color + " " + size;
---

<svg
  class={iconClass}
  aria-hidden="true"
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 14 10"
>
  <path
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="2"
    d="M1 5h12m0 0L9 1m4 4L9 9"></path>
</svg>
