{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@types/*": [
        "src/Types/"
      ],
      "@utils/*": [
        "src/utils/"
      ]
    }
  }
}
